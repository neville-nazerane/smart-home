<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="SmartHome.MobileApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:SmartHome.MobileApp"
    xmlns:page="clr-namespace:SmartHome.MobileApp.Pages"
    Shell.FlyoutBehavior="Flyout">

    <ShellContent
        Title="Scenes"
        ContentTemplate="{DataTemplate page:ScenesPage}"
        Route="scenes" />

    <ShellContent
        Title="Computer"
        ContentTemplate="{DataTemplate page:ComputerPage}"
        Route="computer" />

</Shell>
